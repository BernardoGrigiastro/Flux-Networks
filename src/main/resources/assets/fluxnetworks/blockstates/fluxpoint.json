{
  "multipart": [
    {
      "apply": {
        "model": "fluxnetworks:block/fluxpoint"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "fluxnetworks:block/fluxconnection"
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "fluxnetworks:block/fluxconnection",
        "x": 180
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "fluxnetworks:block/fluxconnection",
        "x": 90,
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "fluxnetworks:block/fluxconnection",
        "x": 90,
        "y": -90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "fluxnetworks:block/fluxconnection",
        "x": 90,
        "y": 0
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "fluxnetworks:block/fluxconnection",
        "x": 90,
        "y": 90
      }
    }
  ]
}